name:           jsonyaml
version:        0.1
cabal-version:  >=1.2
build-type:     Simple
author:         rqiu
data-files:     
                json2yaml.hs, 
                yaml2json.hs

executable json2yaml
  build-depends:   base >= 4
  hs-source-dirs:  src
  ghc-options:     -Wall -rtsopts

executable yaml2json
  build-depends:   base >= 4
  hs-source-dirs:  src
  ghc-options:     -Wall -rtsopts

